<%= form_for(@profile) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<%= f.label :phone_number %>
	<%= f.text_field :phone_number %>
	<%= f.label :seat_number %>
	<%= f.number_field :seat_number, :disabled => @profile.persisted? %>


	<%= f.label :facebook %>
	<%= f.text_field :facebook %>
	<%= f.label :twitter %>
	<%= f.text_field :twitter %>
	<%= f.label :github %>
	<%= f.text_field :github %>
	<%= f.label :blog %>
	<%= f.text_field :blog %>
	<%= f.submit "올리기" %>
<% end %>
